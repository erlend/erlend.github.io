{
  "src/_assets/*": { "type": "asset" },
  "src/_assets/javascripts/*.js": { "type": "javascript" },
  "src/_assets/javascripts/controllers/*_controller.js": {
    "type": "controller",
    "template": [
      "import {open} Controller {close} from \"@hotwired/stimulus\"",
      "",
      "export default class extends Controller {open}",
      "  initialize() {open}",
      "    console.log(\"{hyphenate} initialized\")",
      "  {close}",
      "{close}"
    ]
  },
  "src/_assets/stylesheets/application.css": { "type": "stylesheet" },
  "src/_assets/stylesheets/*.css": { "type": "stylesheet" },
  "src/_data/*": { "type": "data" },
  "src/_data/locales/*.yml": { "type": "locale" },
  "src/_includes/*": { "type": "include" },
  "src/_layouts/*.html": { "type": "layout" },
  "src/_plugins/*.rb": { "type": "plugin" },
  "src/_posts/*.md": {
    "type": "post",
    "template": [
      "---",
      "title: {blank|capitalize}",
      "---"
    ]
  },
  "*": { "start": "bin/dev up --build --detach" }
}
